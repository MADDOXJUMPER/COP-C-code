#include<stdio.h>
int main()
{
    char ch;
    int age; 
    int ID;
    int grade1;
    int grade2;
    int grade3;

    printf("Student Rating of Semester (A/B/C):");
    scanf("%ch",ch)

    printf("Student ID:");
    scanf("%d",ID);

    printf("Age:");
    scanf("%d",age);

    printf("Grade 1:");
    scanf("%d",grade1);

    printf("Grade 2:");
    scanf("%d", grade2)

    printf("Grade 3:");
    scanf("%d", grade3);


}